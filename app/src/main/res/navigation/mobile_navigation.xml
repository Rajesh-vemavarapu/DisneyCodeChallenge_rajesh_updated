<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_select_guests">

    <fragment
        android:id="@+id/navigation_select_guests"
        android:name="com.sample.disneycodechallenge_rajesh.ui.fragments.selectguestfragment.SelectGuestsFragment"
        android:label="@string/select_guests"
        tools:layout="@layout/fragment_select_guests">

        <action
            android:id="@+id/action_navigation_select_guests_to_navigation_conflicts"
            app:destination="@id/navigation_conflicts" />

    </fragment>

    <fragment
        android:id="@+id/navigation_conflicts"
        android:name="com.sample.disneycodechallenge_rajesh.ui.fragments.conflictfragment.ConflictFragment"
        android:label="@string/conflict" />
</navigation>